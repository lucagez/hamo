  "license": "ISC",
  "devDependencies": {
    "ava": "^2.1.0",
    "benchmark": "^2.1.4",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.3",
    "gzip-size-cli": "^3.0.0",
    "microbundle": "^0.11.0",
    "nodemon": "^1.19.1",
    "uglify-es": "^3.3.9"
  }
